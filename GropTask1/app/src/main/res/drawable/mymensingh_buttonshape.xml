<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:topLeftRadius="35dp"
        android:topRightRadius="35dp"
        android:bottomLeftRadius="5dp" />
    <gradient
        android:angle="270"
        android:centerX="35%"
        android:centerColor="#FA234E"
        android:startColor="#E8E8E8"
        android:endColor="#81C92E"
        android:type="linear"
        />
    <padding
        android:left="5dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="1dp"
        />
    <size
        android:width="250dp"
        android:height="60dp"
        />
    <stroke
        android:width="3dp"
        android:color="#63874C"
        />
</shape>